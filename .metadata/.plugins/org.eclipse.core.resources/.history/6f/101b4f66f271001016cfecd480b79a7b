package core;

public class Forsecond {

	public static void main(String[] args) {
	
		
		
		
		for(int a=1;a<5;a++)
		{
			System.out.println(a);
		}
		System.out.println("**************");
		for(int a=1;a<5;a++)
		{
			System.out.println(a);
		}
		for(int a=1;a<5;a++)
		{
			System.out.println(a);
		}

}
}